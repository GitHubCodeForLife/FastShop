<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">

<!-- jQuery library -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

<!-- Popper JS -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>

<!-- Latest compiled JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>

{{#if promotion}}
<div class="container mt-5 mb-5 jumbotron">
    <div class="row text-left">
        <div class="col-12 col-md-7 container">
            <h2>Title </h2>
            <h3 class="mb-0">
                {{promotion.TITLE}}
            </h3>
            <p class="card-text mb-auto">{{promotion.CONTENT}}</p>
        </div>
        <div class="col-12 col-md-5 container">
            <img src="{{promotion.IMAGE}}"alt="Image of Promotion" >
        </div>
    </div>
</div>
{{/if}}
{{#unless promotion}}
<h1>Error</h1>
{{/unless}}





<!-- Javascript-->
<script src="js/core.min.js"></script>
<script src="js/script.js"></script>